package com.org.domain;

public class Employee {
	
	private Integer id;
	
	private String name;
	
	private String address;
	
	public Employee() {
	System.out.println("Emp 0-param..");
	}

	public Employee(Integer id, String name, String address) {
		this.id = id;
		this.name = name;
		this.address = address;
	}
	

}
